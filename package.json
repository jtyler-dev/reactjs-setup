{
  "name": "reactjs-setup",
  "version": "1.0.0",
  "description": "simple setup project to help get you started developing with react.js",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git@jtyler-dev:jtyler-dev/reactjs-setup.git"
  },
  "keywords": [
    "react.js",
    "setup",
    "help",
    "react",
    "boilerplate"
  ],
  "author": "jtyler",
  "license": "ISC",
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  }
}
